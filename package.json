{
    "name": "@pyro/admin-theme",
    "version": "1.0.0",
    "scripts": {},
    "main": "lib/index.ts",
    "typings": "lib/index.ts",
    "types": "lib/index.ts",
    "sass": "lib/admin-theme.scss",
    "pyro": {
        "srcPath": "lib",
        "entrypoints": [
            {
                "path": "index.ts",
                "provider": "AdminThemeServiceProvider"
            },
            {
                "path": "inertia/index.ts",
                "suffix": "inertia",
                "provider": "AdminThemeInertiaServiceProvider"
            },
            {
                "path": "livewire/index.ts",
                "suffix": "livewire",
                "provider": "AdminThemeLivewireServiceProvider"
            }
        ]
    },
    "dependencies": {
        "@ag-grid-community/all-modules": "^22.1.1",
        "@ag-grid-community/vue": "^22.1.2",
        "@bem/sdk.naming.entity": "^0.2.11",
        "@mdi/font": "^4.7.95",
        "@mdi/svg": "^4.7.95",
        "vue-directive-tooltip": "^1.6.3",
        "@pyro/platform": "^1.0.0",
        "@radic/observable": "^1.0.0",
        "@radic/tree": "^1.0.0",
        "@types/classnames": "^2.2.9",
        "@types/popper.js": "^1.11.0",
        "bootstrap": "^4.3.1",
        "classnames": "^2.2.6",
        "csstip": "^0.0.0",
        "csstips": "^1.2.0",
        "csx": "^10.0.0",
        "element-ui": "^2.12.0",
        "font-awesome": "^4.7.0",
        "foundation-sites": "^6.5.3",
        "popper.js": "^1.16.0",
        "portal-vue": "^2.1.7",
        "sass-ignis": "^1.0.3",
        "typestyle": "^2.0.4",
        "vue-avatar": "^2.1.8",
        "vue-bem-cn": "^1.1.1",
        "vue-clickaway": "^2.2.2",
        "vue-good-table": "^2.18.1",
        "lodash-decorators": "^6.0.1",
        "vue-popperjs": "^2.2.0",
        "vuescroll": "^4.14.4",
        "vuetable-2": "next"
    },
    "devDependencies": {}
}
